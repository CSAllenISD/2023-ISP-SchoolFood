<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.schoolfood.ui.home.SubwayFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.schoolfood.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_prevordered"
        android:name="com.schoolfood.ui.previousordered.PreviousOrderedFragment"
        android:label="@string/title_prevordered"
        tools:layout="@layout/fragment_previousordered" />

    <fragment
        android:id="@+id/navigationd_subway"
        android:name="com.schoolfood.ui.subway.SubwayFragment"
        android:label="@string/title_prevordered"
        tools:layout="@layout/fragment_previousordered" />
</navigation>